

<div class = "well">
   

   <%= form_with url: course_materials_path(), html: { multipart: true } do |f| %>
   
  
      <%= f.label :title %>
      <%= f.text_field :title %><br>
      <%= f.label :content %>
      <%= f.text_field :content %><br>
      <%= f.label :attachment %><br>
      <%= f.file_field :attachment %><br>
      <%= f.submit "Save", class: "btn btn-primary" %>
   <% end %>
</div>